
## Server Settings
# The domain name where Vaultwarden will be accessible from
DOMAIN=https://vault.example.com

# Rocket server address
# ROCKET_ADDRESS=0.0.0.0

# Rocket server port
# ROCKET_PORT=8989

# Enable WebSocket notifications
WEBSOCKET_ENABLED=true

## Admin Settings
# Admin token for the admin page (generate with: `openssl rand -base64 48`)
ADMIN_TOKEN=CHANGE_ME_VERY_IMPORTANT

# Disable the admin page (true/false)
# DISABLE_ADMIN_TOKEN=false

## Database Settings
# Database URL
# DATABASE_URL=data/db.sqlite3

# Database connection max lifetime in seconds (default 30)
# DATABASE_MAX_CONNS=10

## Email Settings
# SMTP server for sending emails
# SMTP_HOST=smtp.example.com

# SMTP server port
# SMTP_PORT=587

# Use SSL for SMTP
# SMTP_SSL=true

# Use explicit TLS for SMTP
# SMTP_EXPLICIT_TLS=false

# SMTP username
# SMTP_USERNAME=username

# SMTP password
# SMTP_PASSWORD=password

# SMTP sender address
# SMTP_FROM=vaultwarden@example.com

# SMTP FROM header display name
# SMTP_FROM_NAME=Vaultwarden

## Security Settings
# Enable HTTPS (true/false)
# ROCKET_TLS={certs="path/to/cert.pem",key="path/to/key.pem"}

# Allow new signups (true/false)
# SIGNUPS_ALLOWED=true

# Invite organization members when they use the mobile apps
# INVITATIONS_ALLOWED=true

# Force users to use stronger passwords
# PASSWORD_HINTS_ALLOWED=true

# Enable web vault (true/false)
# WEB_VAULT_ENABLED=true

# HIBP API check new passwords
# HIBP_API_KEY=your_key_here

## Logging
# Log level (off, error, warn, info, debug, trace)
LOG_LEVEL=warn

# Send logs to syslog
# USE_SYSLOG=false

## Advanced Settings
# Set the lifetime of the emergency access invitation before it expires
# EMERGENCY_ACCESS_ALLOWED=true

# Uncomment to require authentication for API calls
# REQUIRES_AUTH=true

# Disable icon downloads 
# DISABLE_ICON_DOWNLOAD=false

# Set to true to enable extended logging
# EXTENDED_LOGGING=true

# Browser cache time for attachments in seconds
# ATTACHMENTS_CACHE_TTL=2592000

